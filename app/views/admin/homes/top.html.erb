<h2>写真一覧（全<%= @photos.count %>枚）</h2>


<% @photos.each do |photos| %>

<%= link_to admin_photo_path(photos.id) do %>
<%= image_tag photos.get_image(100,100) %>
<% end %><br>
<%= link_to admin_customer_path(photos.id) do %>
<%= photos.customer.nickname %><br>
<% end %>

<% end %>

<%= paginate @photos %>